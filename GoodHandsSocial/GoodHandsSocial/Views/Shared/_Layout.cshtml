                                    <div>Gerenciamento de Conta</div>
                                </div>
                            </div>
                        </div>
                        <div class="drawer-spacer bg-body-bg">
                        </div>
                        <!-- MENU -->
                        <ul class="drawer-menu" id="userMenu" data-children=".drawer-submenu">
                            <li class="drawer-menu-item">
                                <a asp-area="Identity" asp-page="/Account/Manage/Index">
                                    <i class="material-icons">lock</i>
                                    <span class="drawer-menu-text"> Account </span>
                                </a>
                            </li>
                            <li class="drawer-menu-item">
                                <a asp-area="Identity" asp-page="/Account/Login">
                                    <i class="material-icons">account_circle</i>
                                    <span class="drawer-menu-text">Login</span>
                                </a>
                            </li>
                            <li class="drawer-menu-item">
                                <a asp-area="Identity" asp-page="/Account/Register">
                                    <i class="material-icons">account_circle</i>
                                    <span class="drawer-menu-text">Register</span>
                                </a>
                            </li>
                            <li class="drawer-menu-item">
                                <a asp-area="Identity" asp-page="/Account/Logout">
                                    <i class="material-icons">exit_to_app</i>
                                    <span class="drawer-menu-text">Logout</span>
                                </a>
                            </li>
                        </ul>
                    </nav>
